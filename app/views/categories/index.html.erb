<nav class='navbar'>
  <a href="#"><i class="fa-solid fa-bars"></i></a>
  <a class='nav-title' href="#">TRANSACTIONS</a>
  <a href="#"><i class="fa fa-search search_icon" aria-hidden="true"></i></a>
</nav>
<div class='tran-background'>
<p style="color: green"><%= notice %></p>
<div id="categories">
  <% @categories.each do |category| %>
      <% if can? :manage, category%>
    <%= link_to category_path(category.id), class:'text-decoration-none' do %>
    <div>
    <%= render category %>
    </div>
  <% end %>  
  <% end %>
  <% end %>
</div>
</div>

<div class='logout-box d-flex flex-column justify-content-center align_items_center'>
<div class="logout-sub-box">
<%= link_to "New category", new_category_path, class:'log-out' %>
<%= button_to 'Log Out', destroy_user_session_path, class:'log-out', method: :delete%>
</div>
</div>